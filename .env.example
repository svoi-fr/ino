# ===== DATABASE CONFIGURATION =====

# Local MongoDB (Docker)
MONGO_HOST=localhost
MONGO_PORT=27017
MONGO_USERNAME=inoadmin
MONGO_PASSWORD=your_secure_password_here
MONGO_DB=ino_refugee_db

# Local Milvus (Docker)
MILVUS_HOST=localhost
MILVUS_PORT=19530

# Remote Vector DB (Optional)
WEAVIATE_URL=your_weaviate_url
WEAVIATE_API_KEY=your_weaviate_api_key

# ===== API KEYS =====

# LLM API Keys (choose the ones you need)
MISTRAL_API_KEY=your_mistral_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
OPENAI_API_KEY=your_openai_api_key
COHERE_API_KEY=your_cohere_api_key

# Embeddings
VOYAGE_API_KEY=your_voyage_api_key

# Google API Key (optional)
GOOGLE_API_KEY=your_google_api_key

# ===== TELEGRAM CONFIGURATION =====
# Get these from https://my.telegram.org/apps
TELEGRAM_API_ID=your_telegram_api_id
TELEGRAM_API_HASH=your_telegram_api_hash
TELEGRAM_PHONE_NUMBER=your_phone_number
TELEGRAM_TEST_GROUP=test_group_name

# ===== DEVELOPMENT & TESTING =====
# Set to true to use mock data for development
USE_MOCK_DB=false

# ===== WEB INTERFACE =====
# Mongo Express basic auth
ME_CONFIG_BASICAUTH_USERNAME=admin_username
ME_CONFIG_BASICAUTH_PASSWORD=admin_password